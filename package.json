{
  "name": "cf-cdn-optimizer",
  "version": "0.1.28",
  "description": "Cloudflare CDN 优选加速管理平台 - 自动化管理 Cloudflare 自定义主机名 + 阿里云 DNS 优选 IP",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "build:all": "npm run build && npm run package",
    "start": "node server/index.js",
    "install:all": "npm install && cd client && npm install",
    "migrate": "node server/database/migrate.js",
    "package": "pkg . --compress GZip"
  },
  "bin": "server/index.js",
  "pkg": {
    "assets": [
      "client/dist/**/*",
      "node_modules/sqlite3/**/*",
      "node_modules/cloudflare/**/*",
      "node_modules/@alicloud/**/*"
    ],
    "targets": [
      "node18-linux-x64"
    ],
    "outputPath": "dist"
  },
  "keywords": [
    "cloudflare",
    "cdn",
    "dns",
    "aliyun",
    "optimization"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "body-parser": "^1.20.2",
    "cloudflare": "^3.3.0",
    "@alicloud/alidns20150109": "^3.0.2",
    "@alicloud/openapi-client": "^0.4.8",
    "@alicloud/tea-util": "^1.4.7",
    "sqlite3": "^5.1.7",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "axios": "^1.6.7",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.3",
    "concurrently": "^8.2.2",
    "pkg": "^5.8.1"
  }
}
